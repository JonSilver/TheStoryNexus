{
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "importOrder": ["^@/(.*)$", "^[./]"],
    "importOrderSeparation": false,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
